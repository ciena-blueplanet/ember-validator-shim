{
  "name": "ember-validator-shim",
  "version": "0.1.1",
  "description": "String validators and sanitizers for Ember apps",
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "scripts": {
    "build": "ember build",
    "lint": "npm run lint-js && npm run lint-md",
    "lint-js": "eslint *.js addon app blueprints config tests",
    "lint-md": "remark *.md",
    "start": "ember server",
    "test": "npm run lint && ember test"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ciena-blueplanet/ember-validator-shim.git"
  },
  "homepage": "https://github.com/ciena-blueplanet/ember-validator-shim",
  "engines": {
    "node": ">= 5.0.0"
  },
  "bugs": {
    "url": "https://github.com/ciena-blueplanet/ember-validator-shim/issues"
  },
  "author": "Matthew Dahl (https://github.com/sandersky)",
  "license": "MIT",
  "devDependencies": {
    "broccoli-asset-rev": "^2.4.2",
    "ember-cli": "^2.5.1",
    "ember-cli-app-version": "^1.0.0",
    "ember-cli-dependency-checker": "^1.2.0",
    "ember-cli-htmlbars-inline-precompile": "^0.3.1",
    "ember-cli-inject-live-reload": "^1.4.0",
    "ember-cli-mocha": "0.10.2",
    "ember-cli-uglify": "^1.2.0",
    "ember-disable-prototype-extensions": "^1.1.0",
    "ember-export-application-global": "^1.0.5",
    "ember-load-initializers": "^0.5.1",
    "ember-resolver": "^2.0.3",
    "ember-try": "0.2.2",
    "eslint": "^2.11.1",
    "eslint-config-frost-standard": "^2.0.0",
    "loader.js": "^4.0.1",
    "remark": "^4.2.2",
    "remark-lint": "^4.0.0",
    "validator": "5.4.0"
  },
  "keywords": [
    "ember-addon",
    "validation",
    "validator"
  ],
  "dependencies": {
    "5to6-codemod": "^1.1.0",
    "broccoli-metal": "^1.0.0",
    "broccoli-replace": "^0.12.0",
    "ember-cli-babel": "^5.1.6",
    "ember-cli-htmlbars": "^1.0.1",
    "jscodeshift": "^0.3.25"
  },
  "ember-addon": {
    "configPath": "tests/dummy/config"
  }
}