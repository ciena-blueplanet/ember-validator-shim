{
  "name": "ember-validator-shim",
  "version": "2.1.2",
  "description": "String validators and sanitizers for Ember apps",
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "scripts": {
    "build": "ember build",
    "lint": "lint-all-the-things",
    "start": "ember server",
    "test": "npm run lint && npm run test-addon",
    "test-addon": "EMBER_TRY_SCENARIO=${EMBER_TRY_SCENARIO:=default} && ember try:one $EMBER_TRY_SCENARIO --- COVERAGE=true ember test"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ciena-blueplanet/ember-validator-shim.git"
  },
  "homepage": "https://github.com/ciena-blueplanet/ember-validator-shim",
  "engines": {
    "node": ">= 5.0.0"
  },
  "bugs": {
    "url": "https://github.com/ciena-blueplanet/ember-validator-shim/issues"
  },
  "author": "Matthew Dahl (https://github.com/sandersky)",
  "license": "MIT",
  "devDependencies": {
    "broccoli-asset-rev": "^2.5.0",
    "ember-cli": "^2.11.0",
    "ember-cli-app-version": "^2.0.1",
    "ember-cli-chai": "^0.3.2",
    "ember-cli-dependency-checker": "^1.3.0",
    "ember-cli-htmlbars-inline-precompile": "0.3.6",
    "ember-cli-inject-live-reload": "^1.4.1",
    "ember-cli-mocha": "0.13.2",
    "ember-cli-test-loader": "^1.1.1",
    "ember-cli-uglify": "^1.2.0",
    "ember-disable-prototype-extensions": "^1.1.0",
    "ember-export-application-global": "^1.1.1",
    "ember-load-initializers": "0.6.3",
    "ember-resolver": "^2.1.1",
    "ember-test-utils": "^1.10.3",
    "loader.js": "^4.1.0",
    "validator": "6.2.1"
  },
  "keywords": [
    "ember-addon",
    "validation",
    "validator"
  ],
  "dependencies": {
    "5to6-codemod": "^1.1.0",
    "broccoli-merge-trees": "^1.1.4",
    "broccoli-metal": "^1.0.0",
    "broccoli-replace": "^0.12.0",
    "ember-cli-babel": "^5.1.9",
    "ember-cli-htmlbars": "^1.1.0",
    "jscodeshift": "^0.3.25"
  },
  "ember-addon": {
    "configPath": "tests/dummy/config"
  }
}
